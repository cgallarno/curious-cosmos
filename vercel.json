{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
  ],
  "builds": [
    {
      "src": "server/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"

  ]
}

